{
  "name": "open-analyzer",
  "description": "Open Analyzer for analyzing logs from Open Education platform",
  "scripts": {
    "start": "run-p start:dev start:api",
    "start:dev": "webpack-dev-server --config webpack.config.dev.js --port 3000",
    "prestart:api": "node tools/createMockDb.js",
    "start:api": "node tools/apiServer.js",
    "prettiercheck": "prettier --check \"{src,tools}/**/*.{js,json,css}\" \"!package-lock.json\"",
    "prettierwrite": "prettier --write \"{src,tools}/**/*.{js,json,css}\" \"!package-lock.json\"",
    "prettier": "prettier",
    "lint": "npx eslint . --cache --ext .js && npm run prettiercheck && echo 'Success! No linting errors found!'",
    "lint:fix": "npx eslint . --cache --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@trendmicro/react-iframe": "^1.0.1",
    "@trendmicro/react-sidenav": "^0.5.0",
    "antd": "3.26.11",
    "axios": "0.21.1",
    "bootstrap": "4.3.1",
    "file-loader": "^6.0.0",
    "immer": "2.1.3",
    "lodash": "4.17.19",
    "prettier": "2.0.5",
    "prop-types": "15.7.2",
    "react": "16.8.4",
    "react-bootstrap": "1.0.0-beta.16",
    "react-click-outside": "github:tj/react-click-outside",
    "react-dom": "16.8.4",
    "react-redux": "6.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "5.0.0",
    "react-toastify": "4.5.2",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "resolve-url": "^0.2.1",
    "url-loader": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-preset-react-app": "7.0.2",
    "css-loader": "^2.1.1",
    "cssnano": "4.1.10",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "eslint": "5.15.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-lodash": "7.1.0",
    "eslint-plugin-mocha": "7.0.1",
    "eslint-plugin-notice": "0.9.10",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "4.0.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fetch-mock": "7.3.1",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.9.0",
    "ignore-styles": "5.0.1",
    "jest": "24.5.0",
    "json-server": "0.14.2",
    "lint-staged": "10.2.10",
    "mini-css-extract-plugin": "0.5.0",
    "node-fetch": "2.6.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "4.1.5",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "16.8.4",
    "react-testing-library": "6.0.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "rimraf": "2.6.3",
    "sass-loader": "^9.0.2",
    "style-loader": "^0.23.1",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1"
  },
  "prettier": {
    "bracketSpacing": false,
    "endOfLine": "lf",
    "jsxBracketSameLine": true,
    "jsxSingleQuote": false,
    "parser": "",
    "printWidth": 180,
    "semi": true,
    "tabWidth": 4,
    "overrides": [
      {
        "files": "*.json",
        "excludeFiles": [
          "package-lock.json"
        ],
        "options": {
          "parser": "json",
          "tabWidth": 4
        }
      },
      {
        "files": [
          "package-lock.json"
        ],
        "options": {
          "parser": "json",
          "tabWidth": 2
        }
      },
      {
        "files": "*.css",
        "options": {
          "parser": "css",
          "tabWidth": 2
        }
      }
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  }
}
